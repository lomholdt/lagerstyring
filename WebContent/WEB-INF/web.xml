<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>lagerstyring</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PageController</servlet-name>
    <servlet-class>com.lomholdt.lagerstyring.controller.PageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PageController</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutController</servlet-name>
    <servlet-class>com.lomholdt.lagerstyring.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutController</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddInventoryController</servlet-name>
    <servlet-class>com.lomholdt.lagerstyring.controller.AddInventoryController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddInventoryController</servlet-name>
    <url-pattern>/inventory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OpenStorageController</servlet-name>
    <servlet-class>com.lomholdt.lagerstyring.controller.OpenStorageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OpenStorageController</servlet-name>
    <url-pattern>/open</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>InventoryCountController</servlet-name>
    <servlet-class>com.lomholdt.lagerstyring.controller.InventoryCountController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InventoryCountController</servlet-name>
    <url-pattern>/count</url-pattern>
  </servlet-mapping>
  
  
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.css</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.png</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.js</url-pattern>
  </servlet-mapping>
</web-app>